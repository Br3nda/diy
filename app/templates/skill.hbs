<h1>{{model.title}}</h1>
<img src="{{model.images.medium}}">

<p>{{model.description}}</p>

{{outlet}}

<ol>
{{#each challenge in challenges}}
  <h2>{{challenge.title}}</h2>
  <p><img src="{{challenge.image.web_300.url}}" height="{{challenge.image.web_300.height}}" width="{{challenge.image.web_300.width}}"/></p>
  <p>{{challenge.description}}</p>
{{/each}}
</ol>