<div class="skills-skill">
  <h1>{{model.title}}</h1>
  <img class="skill-image" src="{{model.images.medium}}" class="pure-img-responsive" />

  <p>{{model.description}}</p>
  {{outlet}}
  <ol>
  {{#each challenge in challenges}}
    {{challenge-details challenge=challenge}}
  {{/each}}
  </ol>
</div>